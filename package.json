{
  "name": "camerakit-nest-api",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "migrations:generate": "node_modules/ts-node/dist/bin.js node_modules/.bin/typeorm migration:generate",
    "migrations:run": "node_modules/ts-node/dist/bin.js node_modules/.bin/typeorm migration:run",
    "migrations:revert": "node_modules/ts-node/dist/bin.js node_modules/.bin/typeorm migration:revert",
    "start:dev": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.2.2",
    "@nestjs/core": "^5.2.2",
    "@nestjs/graphql": "^3.0.0",
    "@nestjs/passport": "^1.1.0",
    "@nestjs/typeorm": "^5.1.0",
    "@types/graphql": "^14.0.1",
    "@types/nodemailer": "^4.6.5",
    "apollo-server-express": "^2.0.3",
    "aws-sdk": "^2.324.0",
    "bcrypt-nodejs": "^0.0.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "email-templates": "^5.0.0",
    "express": "^4.16.3",
    "express-sslify": "^1.2.0",
    "graphql": "^0.13.2",
    "helmet": "^3.14.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "stripe": "^6.8.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.1.1",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.2.2",
    "@types/express": "^4.0.39",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.1",
    "@types/supertest": "^2.0.4",
    "jest": "^23.5.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.5.0",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
